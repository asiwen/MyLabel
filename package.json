{
  "name": "mylabel",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "build":{
    "appId": "mylabel",
    "win": {
      "target": ["nsis", "zip"]
    }
  },
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ mylabel --out ./dist --overwrite",
    "dist": "electron-builder --win --x64"
  },
  "repository": "https://github.com/asiwen/MyLabel",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Tranq",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
